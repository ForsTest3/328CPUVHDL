LIBRARY ieee;
USE ieee.std_logic_1164.all;

entity registeelB IS
port (B : in std_logic_vector(7 downto 0) ; 
res, clk : in std_logic ;
Q : out std_logic_vector(7 downto 0)) ; 
end registeelB;

architecture behavior of registeelB is
begin
process (res, clk)
begin
if res = '1' then
Q <= "00000000" ;
elsif (clk'EVENT AND clk = '1') then
Q <= B ;
end if ;
end process ;
end behavior; 